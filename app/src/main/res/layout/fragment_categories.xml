        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/lay4">

        <com.dev.amr.amlakfile.data.model.custom_views.ITextView
            android:id="@+id/txt_project_sakhtosaz"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:padding="15dp"
            android:text="@string/txt_projects_sakhtosaz"
            android:textSize="20sp"
            app:drawableLeftCompat="@drawable/arrow_back_icon"
            app:layout_constraintStart_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <!--        <androidx.constraintlayout.widget.ConstraintLayout-->
        <!--            android:layout_width="match_parent"-->
        <!--            android:layout_height="2dp"-->
        <!--            android:layout_marginEnd="16dp"-->
        <!--            android:layout_marginStart="16dp"-->
        <!--            android:background="@drawable/shape_line"-->
        <!--            app:layout_constraintStart_toStartOf="@id/txt_project_sakhtosaz"-->
        <!--            app:layout_constraintEnd_toEndOf="@id/txt_project_sakhtosaz"-->
        <!--            app:layout_constraintTop_toBottomOf="@id/txt_project_sakhtosaz"/>-->

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>

